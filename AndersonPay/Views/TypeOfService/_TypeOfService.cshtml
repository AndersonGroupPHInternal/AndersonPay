@{
    ViewBag.Title = "_TypeOfService";
}

<div class="page_TypesofService">
<div class="form-group">
        <label for="Type Of Service Id"></label>
        <input type="hidden" class="form-control" id="TypeOfServiceId" name="TypeOfServiceId"  value="@Model.TypeOfServiceId">
 </div>

<div class="form-group">
    <label for="NameOfService">Name Of Service</label>
    <input type="text" class="form-control" autocomplete="off" id="Name" name="Name" placeholder="Name Of Service" value="@Model.Name">
</div>

<div class="form-group">
    <label for="ServiceDescription">Service Description</label>
    <input type="text" class="form-control" autocomplete="off" id="Description" name="Description" placeholder="Service Description" value="@Model.Description">
</div>

<div class="form-group">
    <input type="submit" class="btn btn-primary" value="Submit" />
</div>
</div>