@{
    ViewBag.Title = "Index";
}
<div class="pageDashboard" ng-controller="TypeOfServiceController as model" ng-init="model.Initialise()">
    <div class="row">
        <div class="col-sm-4">
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <h4>Type of Service</h4>
            <table class="table table-hover table-sm table-bordered ">
                <thead>
                    <tr class="thead-inverse">
                        <th>Name Of Service</th>
                        <th>Service Description</th>
                        <th>Availed</th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="clickableRow" ng-repeat="typeOfService in model.TypeOfServices">
                        <td><span ng-bind="typeOfService.Name"></span></td>
                        <td><span ng-bind="typeOfService.Description"></span></td>
                        <td><span ng-bind="typeOfService.Service.Availed"></span></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div>
        <canvas id="myChart" height="900"></canvas>
    </div>
</div>